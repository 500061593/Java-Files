package jvlabexp1;
class A
{
void display()
{ System.out.println("a");
}
}
 
class B extends A
{
void display()
{ System.out.println("b");
}
}
 
class C extends A
{
void display()
{ System.out.println("c");
}
}
 
public class main
{ public static void main(String args[])
{ A a;
a = new B();
a.display();
a = new C();
a.display();
}
}